{% block title %}Model Example : Mixture + Bourbon{% endblock %}
{% block content %}
	<h1>{{ model.title }}</h1>

	<p class="intro">{{ model.description }}</p>

		<ul class="clients">
			{% for client in model.clients %}
			<li><img src="{{ client.img }}" />
			</br>{{ client.name }}</li>
			{% endfor %}
		</ul>


{% endblock %}
