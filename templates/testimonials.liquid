{% block title %}{% endblock %}
{% block content %}
	<h1>{{ model.title }}</h1>

	<p class="intro">{{ model.description }}</p>

	{% for quote in model.quotes %}

		<article class="quote">
			<p class="bold">{{ quote.quote }}</p>
			<p>{{ quote.name}}&nbsp;/&nbsp;{{ quote.position }}&nbsp;/&nbsp;{{ quote.company }}</p>
		</article>

	{% endfor %}

{% endblock %}
